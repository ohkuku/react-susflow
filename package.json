    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
